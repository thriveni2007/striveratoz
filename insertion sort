package thriveni;
import java.util.*;
//insertion sort
public class Linkedlist1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      Scanner sc=new Scanner(System.in);
      int n=5;
      int min,j;
      int a[]=new int[] {10,7,3,2,6};
      int temp;
      for(int i=0;i<=n-1;i++) {
      for( j=i+1;j<=n-1;j++){
      if(a[i]>a[j]) {
      temp=a[i];
	  a[i]=a[j];
	  a[j]=temp;}
       }}
      for(int k=0;k<n;k++) {
      System.out.print(a[k]);}
 
	
	}

}
