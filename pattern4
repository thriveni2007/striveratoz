class Main {
    public static void main(String[] args) {
        int a=1,b=0,temp;
        for(int i=1;i<=5;i++){
            for(int k=1;k<=i;k++){
                if((i+k)%2==0){
                     System.out.print("1");  
                } 
                else{
                     System.out.print("0");  
                }
            }
             
            System.out.print("\n");  
         
    }}}
